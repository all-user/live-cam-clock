{
  "name": "live-cam-clock",
  "version": "0.0.3",
  "description": "Clock on the Live Cam.",
  "main": "lib/main.bundle.js",
  "config": {
    "electronVersion": "1.2.5",
    "githubUserName": "all-user",
    "packageName": "LiveCamClock",
    "repoName": "live-cam-clock"
  },
  "scripts": {
    "help": "npm-run-help",
    "test:ci": "exit 0",
    "build:html": "jade src/index.jade -P -o ./",
    "watch:html": "jade -w src/index.jade -P -o ./",
    "build:js": "webpack",
    "watch:js": "webpack -w",
    "build": "npm-run-all -p build:*",
    "watch": "npm-run-all -p watch:*",
    "pack:osx": "cross-conf-env electron-packager ./ $npm_package_config_packageName --platform=darwin --arch=x64 --version=$npm_package_config_electronVersion --ignore=\"/node_modules/\" --out=./dist --overwrite",
    "pack": "rimraf dist && mkdir dist && npm-run-all -p pack:*",
    "archive:osx": "rimraf dist/osx && mkdirp dist/osx && cross-conf-env zip -r \"dist/osx/$npm_package_config_packageName-osx.zip\" \"dist/$npm_package_config_packageName-darwin-x64\"",
    "archive": "npm-run-all -p archive:*",
    "release:build": "BABEL_ENV=production npm run build",
    "release": "npm-run-all -s release:build pack archive",
    "start": "electron ./"
  },
  "author": "Keita Okamoto<ok@aaa.email.ne.jp>",
  "license": "MIT",
  "dependencies": {
    "@all-user/ok-blocks": "0.0.7",
    "@all-user/ok-patterns-lines": "0.1.0",
    "key-string": "^0.2.5",
    "lodash": "^4.17.2",
    "moment": "^2.17.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-actions": "^1.0.1",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-unassert": "^2.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.16.0",
    "cpx": "^1.5.0",
    "cross-conf-env": "^1.0.6",
    "css-loader": "^0.26.0",
    "electron-packager": "^8.3.0",
    "electron-prebuilt": "^1.4.8",
    "eslint": "^3.10.2",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "foovar": "0.3.3",
    "jade": "^1.11.0",
    "mkdirp": "^0.5.1",
    "nib": "^1.1.2",
    "npm-run-all": "^3.1.1",
    "npm-run-help": "^1.1.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^1.13.3"
  }
}
